{
  "name": "functions",
  "version": "1.0.0",
  "scripts": {
    "prebuild": "rm -rf lib dist && mkdir -p lib/models dist/models && cp -r node_modules/@vladmandic/human/models/* lib/models/ && cp -r lib/models/* src/models/ && cp -r node_modules/@vladmandic/human/models/* dist/models/",
    "build": "npm run prebuild && tsc",
    "build:watch": "tsc --watch",
    "serve": "npm run build && firebase emulators:start --only functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log",
    "lint": "eslint --ext .js,.ts . --fix"
  },
  "engines": {
    "node": "18"
  },
  "main": "lib/index.js",
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@tensorflow/tfjs-node": "^4.22.0",
    "@vladmandic/human": "^3.3.5",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "firebase-admin": "^13.1.0",
    "firebase-functions": "^6.3.2"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.19",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0",
    "typescript": "^5.3.3"
  },
  "private": true,
  "files": [
    "lib/**/*",
    "models/**/*"
  ]
}
